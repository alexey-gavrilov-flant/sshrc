#!/bin/bash
cmd=$(find.bin htop)
if [ "$cmd" = "" ]; then
  mkdir -p ~/bin
  if [ -n "$PROXY_PORT" ]; then proxy="--socks5-hostname 127.0.0.1:$PROXY_PORT"; fi
  curl $proxy -L "http://127.0.0.1:81/htop" > ~/bin/htop
  chmod +x ~/bin/htop
  cmd=$(find.bin htop)
fi
if [ "$cmd" != "" ]; then
  exec $cmd "$@"
fi
