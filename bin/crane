#!/bin/bash
cmd=$(find.bin crane)
if [ "$cmd" = "" ]; then
  mkdir -p ~/bin
  curl --socks5-hostname 127.0.0.1:$PROXY_PORT -L  https://github.com/google/go-containerregistry/releases/download/v0.20.6/go-containerregistry_Linux_x86_64.tar.gz | tar -xz -C ~/bin/ crane
  cmd=$(find.bin crane)
fi
if [ "$cmd" != "" ]; then
  exec $cmd "$@"
fi
