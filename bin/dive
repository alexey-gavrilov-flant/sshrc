#!/bin/bash
cmd=$(find.bin dive)
if [ "$cmd" = "" ]; then
  mkdir -p ~/bin
  curl --socks5-hostname 127.0.0.1:$PROXY_PORT -L https://github.com/wagoodman/dive/releases/download/v0.12.0/dive_0.12.0_linux_amd64.tar.gz | tar -xz -C ~/bin/ dive
  cmd=$(find.bin dive)
fi
if [ "$cmd" != "" ]; then
  exec $cmd "$@"
fi
